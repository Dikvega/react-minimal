{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "on",
    "linebreak-style": "off",
    "max-len": [1, 120, 2, {"ignoreComments": true}]
  }
}